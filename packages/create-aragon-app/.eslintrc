{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "standard",
    "plugin:prettier/recommended",
    "plugin:ava/recommended"
  ],
  "parser": "babel-eslint",
  "rules": {
    "valid-jsdoc": "off",
    "no-console": "off",
    "linebreak-style": [
      "error",
      "unix"
    ]
  }
}
